module mimaxiang(
input clk,
input [3:0] bomakaiguan,
input rst_n,//key[1]
input yes,//key[2]输入完密码的确认键
input gewei,//key[3]个位确认
input shiwei,//key[4]十位确认
output reg[7:0] led,
output reg [8:0] seg_led1,  //数码管显示1
output reg1ed[8:0] seg_led2  //数码管显示2
);

parameter M = 4'd2;  
parameter N = 4'd2;  //初始密码22


debounce#(.N(3))u1  //调用消抖模块
( 
.clk(clk),
.rst(rst_n),
.key({yes, gewei, shiwei}),
.key_pulse
);



wire yes_d;wire gewei _d;wire shiwei_d;
//key[2]输入完密码的确认键
//key[3]个位确认
//key[4]士位确认
reg [1:0] error_cnt；/7错误次数计数器






